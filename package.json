{
  "name": "funding-asia-group",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-sg-local": "ng serve --port 4400 --host 0.0.0.0",
    "start-my-local": "ng serve --port 4300 --host 0.0.0.0",
    "start-id-local": "ng serve --port 4200 --host 0.0.0.0",
    "start": "ng serve --port 4500",
    "start-id": "ng serve --port 4200",
    "start-my": "ng serve --port 4300",
    "start-sg": "ng serve --port 4400",
    "start-vn": "ng serve --port 4500",
    "serve": "node ./server.js",
    "prebuild-staging": "node ./replaceBuild.js staging",
    "build-staging": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration staging --aot --buildOptimizer=false",
    "build-staging-sourcemaps": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration staging --aot false --sourcemap=true",
    "prebuild-dynamic": "node ./replaceBuild.js dynamic",
    "build-dynamic": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration dynamic --aot --buildOptimizer=false ",
    "build-dynamic-sourcemaps": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration dynamic --aot false --sourcemap=true",
    "prebuild-production": "node ./replaceBuild.js production",
    "build-production": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration production --aot --buildOptimizer=false",
    "build-production-sourcemaps": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration production --aot false --sourcemap=true",
    "prebuild-uat": "node ./replaceBuild.js uat",
    "prebuild-production-id": "node ./replaceBuild.js production-id",
    "build-production-id": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build  --configuration production-id --aot --buildOptimizer=false",
    "build-uat": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --configuration uat --aot --buildOptimizer=false",
    "build": "npm run build-dynamic",
    "build-withstats": "node --max-old-space-size=4096 node_modules/@angular/cli/bin/ng build --aot --stats-json",
    "test": "ng test",
    "test:coverage": "ng test --codeCoverage=true --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint-scss": "sass-lint -c ./sass-lint.yml 'src/assets/scss/*.scss' -v -q",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "minify": "uglifyjs dist/main.bundle.js --screw-ie8 --compress --mangle --output dist/main.bundle.js && uglifyjs dist/polyfills.bundle.js --screw-ie8 --compress --mangle --output dist/polyfills.bundle.js && uglifyjs dist/inline.bundle.js --screw-ie8 --compress --mangle --output dist/inline.bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/cli": "^8.3.9",
    "@angular/common": "^8.0.2",
    "@angular/compiler": "^8.0.2",
    "@angular/compiler-cli": "^8.0.2",
    "@angular/core": "^8.0.2",
    "@angular/forms": "^8.0.2",
    "@angular/http": "^7.2.15",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "^8.0.2",
    "launchdarkly-js-client-sdk": "^2.16.0",
    "@angular/router": "^8.0.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@sentry/cli": "^1.48.0",
    "@types/fullcalendar": "^2.7.44",
    "@types/highcharts": "^5.0.1",
    "@types/jquery": "^3.3.30",
    "@types/lodash": "^4.14.144",
    "@types/node": "^6.14.7",
    "angular2-highcharts": "^0.5.5",
    "angular2-uuid": "^1.1.1",
    "angularfire2": "^5.0.0-rc.7",
    "bootstrap": "^3.3.7",
    "compression": "^1.7.1",
    "core-js": "^2.6.9",
    "express": "^4.16.2",
    "file-saver": "^1.3.3",
    "firebase": "^5.3.1",
    "font-awesome": "^4.7.0",
    "fullcalendar": "^3.4.0",
    "git-rev-sync": "^1.9.1",
    "hammerjs": "^2.0.8",
    "highcharts": "^6.1.0",
    "intl-tel-input": "^13.0.3",
    "jquery": "^3.4.1",
    "json2csv": "^4.2.1",
    "lodash": "^4.17.11",
    "logrocket": "^1.0.3",
    "moment": "^2.18.1",
    "ng2-completer": "^1.5.1",
    "ngx-clipboard": "^12.1.2",
    "ngx-cookie-service": "^2.2.0",
    "ngx-device-detector": "^1.3.6",
    "ngx-dropzone-wrapper": "^4.6.2",
    "ngx-swiper-wrapper": "^4.8.0",
    "ngx-webstorage": "^4.0.1",
    "primeng": "^5.2.6",
    "primeui": "^4.1.15",
    "raven-js": "^3.27.0",
    "replace-in-file": "^3.1.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.5.2",
    "typescript": "~3.4.5",
    "wowjs": "^1.1.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.144",
    "codelyzer": "~5.1.0",
    "coveralls": "~3.0.9",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-junit-reporter": "~2.0.1",
    "protractor": "^6.0.0",
    "rxjs-tslint": "^0.1.7",
    "sass-lint": "^1.13.1",
    "ts-node": "~2.0.0",
    "tslint": "~5.18.0",
    "uglifyjs": "^2.4.11",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  }
}
